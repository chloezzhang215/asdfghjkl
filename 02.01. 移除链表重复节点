# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def removeDuplicateNodes(self, head: ListNode) -> ListNode:
        if head == None:
            return head
        vals = {head.val}
        cur = head
        
        while head.next:
            if head.next.val not in vals:
                vals.add(head.next.val)
                head = head.next
            else:
                head.next = head.next.next
        return cur  
                
