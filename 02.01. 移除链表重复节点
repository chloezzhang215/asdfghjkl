# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def removeDuplicateNodes(self, head: ListNode) -> ListNode:
        if not head:
            return head
        vals = {head.val}
        node = head
        # 枚举前驱节点
        while node.next:
            # 当前待删除节点
            cur = node.next
            if cur.val not in vals:
                vals.add(cur.val)
                node = node.next
            else:
                node.next = node.next.next
        return head
                
