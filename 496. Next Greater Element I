class Solution(object):
    def nextGreaterElement(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: List[int]
        """
        res = []
        l1 = len(nums1)
        
        for i in range(l1):
            index = nums2.index(nums1[i])
            cur = -1
            for j in range(index, len(nums2)):
                if nums1[i] < nums2[j]:
                    cur = nums2[j]
                    break
            
            res.append(cur)
            
        return res
            
                    
