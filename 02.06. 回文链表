# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None
class Solution:
    def isPalindrome(self, head: ListNode) -> bool:
        res = list()

        while head:
            res.append(head.val)
            head = head.next
            
        i = 0
        j = len(res)-1
        while i<j:
            if res[i] == res[j]:
                i+=1
                j-=1
            else:
                return False
        return True




