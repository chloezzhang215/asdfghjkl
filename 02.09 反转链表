# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution(object):
    def reverseList(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        if not head:
            return None
        
        pre = head
        cur = head.next
        
        while cur != None:
            tmp = cur.next
            pre.next = tmp
            cur.next = head
            head = cur
            cur = tmp
            
        return head
            
        
