# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def partition(self, head: ListNode, x: int) -> ListNode:
        if not head: 
            return head

        cur = head.next
        pre = head
        while cur != None:  
            if cur.val < x:
                tmp = cur.next 
                pre.next = tmp
                cur.next = head
                head = cur
                cur = tmp
            else:
                pre = cur
                cur = cur.next
        return head
